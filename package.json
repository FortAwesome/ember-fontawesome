{
  "name": "@fortawesome/ember-fontawesome",
  "description": "Official Ember component for Font Awesome 5",
  "version": "0.1.7",
  "keywords": [
    "ember-addon",
    "fontawesome"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/FortAwesome/ember-fontawesome.git"
  },
  "license": "MIT",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "contributors": [
    "Mike Wilkerson <mwilkerson@gmail.com>",
    "Ilya Radchenko <knownasilya@gmail.com>",
    "Jonathan Johnson <jon.johnson@ucsf.edu>",
    "Rob Madole <robmadole@gmail.com>",
    "Tom Carter <tomc@spiceworks.com>",
    "Julien Guimont <julien.guimont@gmail.com>",
    "Xaser Acheron <github.com/XaserAcheron>",
    "samcic <github.com/samcic>",
    "Josemar Luedke <github.com/josemarluedke>"
  ],
  "homepage": "https://github.com/FortAwesome/ember-fontawesome",
  "scripts": {
    "build": "ember build",
    "lint:js": "eslint .",
    "start": "ember serve",
    "test": "ember test",
    "test:all": "ember try:each",
    "precommit": "npm run lint:js && lint-staged"
  },
  "lint-staged": {
    "README.md": [
      "markdown-toc -i",
      "git add README.md"
    ]
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.0",
    "broccoli-file-creator": "^1.1.1",
    "broccoli-merge-trees": "^2.0.0",
    "broccoli-plugin": "^1.3.0",
    "broccoli-rollup": "^2.0.0",
    "broccoli-source": "^1.1.0",
    "camel-case": "^3.0.0",
    "ember-ast-helpers": "0.3.5",
    "ember-cli-babel": "^6.6.0",
    "ember-cli-htmlbars": "^2.0.3",
    "ember-get-config": "^0.2.4",
    "glob": "^7.1.2",
    "rollup-plugin-node-resolve": "^3.0.2"
  },
  "devDependencies": {
    "@fortawesome/free-brands-svg-icons": "^5.1.0",
    "@fortawesome/free-regular-svg-icons": "^5.1.0",
    "@fortawesome/free-solid-svg-icons": "^5.1.0",
    "broccoli-asset-rev": "^2.7.0",
    "broccoli-viz": "^4.0.0",
    "ember-ajax": "^3.0.0",
    "ember-cli": "~3.2.0",
    "ember-cli-dependency-checker": "^2.0.0",
    "ember-cli-eslint": "^4.2.1",
    "ember-cli-htmlbars-inline-precompile": "^1.0.0",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-qunit": "^4.3.2",
    "ember-cli-shims": "^1.2.0",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-uglify": "^2.0.0",
    "ember-disable-prototype-extensions": "^1.1.2",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^1.1.0",
    "ember-resolver": "^4.0.0",
    "ember-source": "~2.18.0",
    "ember-source-channel-url": "^1.0.1",
    "ember-try": "^0.2.23",
    "eslint-plugin-ember": "^5.0.0",
    "eslint-plugin-node": "^6.0.1",
    "loader.js": "^4.2.3",
    "qunit-dom": "^0.6.2",
    "husky": "^0.14.3",
    "lint-staged": "^7.2.0",
    "markdown-toc": "^1.2.0",
    "broccoli-stew": "^1.5.0"
  },
  "engines": {
    "node": "6.* || 8.* || >= 10.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "defaultBlueprint": "ember-fontawesome"
  }
}
